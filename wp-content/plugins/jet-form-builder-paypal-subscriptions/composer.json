{
    "name": "hug.reed/jet-form-builder-paypal-subscriptions",
    "type": "project",
    "authors": [
        {
            "name": "Oleksandr Ivanenko",
            "email": "sanko200065@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "crocoblock/jfb-addon-core": "^1.0",
        "crocoblock/jfb-addon-core-dev": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Jet_FB_Paypal\\": "includes/"
        }
    },
    "description": "",
    "config": {
        "jfb-core": {
            "replace-namespace": {
                "patterns": [
					"/^.+\\.(php|lock)$/i",
					"/^.+installed\\.json$/i"
				],
				"search": "JFBCore",
                "replace": "JetPaypalCore"
            }
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "DevCoreJFB\\ScriptsManager::init"
        ]
    }
}
